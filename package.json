{
  "name": "motionframework",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "server": "npm run web-bundle; node --harmony_destructuring server/app.js",
    "dev-server": "NODE_ENV=development node --harmony_destructuring server/app.js",
    "dev-server-redux": "NODE_ENV=development REDUX_ENV=development node --harmony_destructuring server/app.js",
    "web-bundle": "webpack --config www/webpack/prod.config.js --progress --colors",
    "native-server": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "ios-bundle": "node node_modules/react-native/local-cli/cli.js bundle     --dev false --entry-file index.ios.js     --platform ios     --bundle-output ios/main.jsbundle",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "android-bundle": "node node_modules/react-native/local-cli/cli.js bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "clear-cache": "gulp clear-cache"
  },
